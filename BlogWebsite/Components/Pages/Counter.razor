@page "/counter"
@inject NavigationManager Navigation

<h3>Tải xuống tệp PDF</h3>

<MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick="DownloadPdf">
    Tải Xuống PDF
</MudButton>

@code {
    private void DownloadPdf()
    {
        var url = "/FilePDF/sample.pdf";
        Navigation.NavigateTo(url, forceLoad: true);
    }
}